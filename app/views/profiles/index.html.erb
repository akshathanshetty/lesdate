<h1>Listing profiles</h1>

<table>
  <tr>
    <th>Email</th>
    <th>Name</th>
    <th>Abturself</th>
    <th>City</th>
    <th>State</th>
    <th>Country</th>
    <th>Stability</th>
    <th>Partnerquality</th>
    <th>Likes</th>
    <th>Dislike</th>
    <th>Hobbies</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @profiles.each do |profile| %>
  <tr>
    <td><%= profile.email %></td>
    <td><%= profile.name %></td>
    <td><%= profile.abturself %></td>
    <td><%= profile.city %></td>
    <td><%= profile.state %></td>
    <td><%= profile.country %></td>
    <td><%= profile.stability %></td>
    <td><%= profile.partnerquality %></td>
    <td><%= profile.likes %></td>
    <td><%= profile.dislike %></td>
    <td><%= profile.hobbies %></td>
    <td><%= link_to 'Show', profile %></td>
    <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
    <td><%= link_to 'Destroy', profile, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Profile', new_profile_path %>
